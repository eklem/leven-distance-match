{
  "name": "leven-match",
  "version": "1.0.0",
  "description": "Return all word matches between two arrays within given Levenshtein distance",
  "main": "index.js",
  "scripts": {
    "compile-for-browser": "webpack",
    "empty-sandbox": "rm -rf test/sandbox && mkdir test/sandbox",
    "test": "standard './*.js' './test/*.js' && npm run empty-sandbox && npm run compile-for-browser && tape ./test/test.js && cat test/sandbox/bundle.js | tape-run && rm -rf test/sandbox"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eklem/leven-match.git"
  },
  "keywords": [
    "search",
    "fuzzy",
    "match",
    "levenshtein",
    "distance"
  ],
  "depencencies": {
    "leven": "^3.1.0"
  },
  "devDependencies": {
    "standard": "^14.3.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "tape": "^5.0.0-next.5",
    "tape-run": "^7.0.0"
  },
  "author": "Espen Klem",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eklem/leven-match/issues"
  },
  "homepage": "https://github.com/eklem/leven-match#readme",
  "dependencies": {
    "leven": "^3.1.0"
  }
}
